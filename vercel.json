{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/companies/search",
      "dest": "/api/edgar"
    },
    {
      "src": "/api/companies/(.*)",
      "dest": "/api/edgar"
    },
    {
      "src": "/api/test",
      "dest": "/api/test"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/edgar"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npx vite build",
  "outputDirectory": "dist",
  "installCommand": "npm install"
}